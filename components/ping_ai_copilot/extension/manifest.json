{
  "manifest_version": 3,
  "name": "Ping ai copilot",
  "version": "1.0",
  "description": "Ai copilot - by Ping.",
  "permissions": [
    "activeTab",
    "scripting",
    "declarativeNetRequest",
    "declarativeNetRequestFeedback",
    "declarativeNetRequestWithHostAccess"
  ],
  "host_permissions": [
    "https://openai-text-summarizer.azurewebsites.net/*"
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["extension/out/ping_ai_copilot_content.bundle.js"],
      "css": ["./content/ui/style.css"]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["./assets/content_copy.svg", "./assets/aiSummarizerIcon.svg"],
      "matches": ["<all_urls>"]
    }
  ],
  "background": {
    "service_worker": "extension/out/ping_ai_copilot_background.bundle.js",
    "type": "module"
  },

  "key": ""
}
