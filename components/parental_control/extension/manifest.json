{
  "manifest_version": 3,
  "name": "Parental Control",
  "version": "1.0",
  "description": "Parental Control extension - by Ping.",
  "permissions": [
    "storage",
    "tabs",
    "activeTab",
    "windows",
    "declarativeNetRequest",
    "declarativeNetRequestFeedback"
  ],
  "background": {
    "service_worker": "extension/out/parental_control_background.bundle.js",
    "type": "module"
  },
  "action": {
    "default_popup": "extension/content/ui/popup.html",
    "default_position": "center",
    "default_icon": {
      "38": "extension/assets/Logo_inactive.png",
      "48": "extension/assets/Logo_active.png",
      "128": "extension/assets/Logo_active.png"
    }
  },

  "icons": {
    "38": "extension/assets/Logo_inactive.png",
    "48": "extension/assets/Logo_active.png",
    "128": "extension/assets/Logo_active.png"
  },
  "key": ""
}